          "executable": false
        }
      },
      "env": {
        "YOUSIGN_BASEURL": "https://api.yousign.app/v3",
        "YOUSIGN_APIKEY": "%generate(secret)%"
      },
      "ref": "58a6b6a626043353dc4f4e8217e957cf0e14fbc6"
    }
  }
}