{
  "manifests": {
    "walidboughdiri/yousign-bundle": {
      "manifest": {
        "bundles": {
          "YousignBundle\\YousignBundle": [ "all" ]
        }
      },
      "files": {
        "config/packages/yousign.yaml": {
          "contents": [
            "parameters:",
            "    env(YOUSIGN_BASEURL): ~",
            "    env(YOUSIGN_APIKEY): ~,",
            "",
            "yousign:",
            "    baseUrl: '%env(YOUSIGN_BASEURL)%'",
            "    apiKey: '%env(YOUSIGN_APIKEY)%'"
